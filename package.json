{
  "name": "ember-cli-dependency-lint",
  "version": "1.0.2",
  "description": "Lint your app's addon dependencies, making sure you only have one version of each.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "salsify/ember-cli-dependency-lint",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "lint": "eslint lib tests-node",
    "test": "yarn run lint && yarn run test-node && yarn run test-browser",
    "test-browser": "ember test",
    "test-node": "mocha tests-node/{unit,acceptance}/**/*-test.js"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "broccoli-plugin": "^1.3.0",
    "chalk": "^2.3.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.6.0",
    "chai": "^4.1.2",
    "console-ui": "^2.1.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "^2.18.1",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-app-version": "^3.1.3",
    "ember-cli-babel": "^6.11.0",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-qunit": "^4.3.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-data": "^2.18.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.5.0",
    "ember-source": "2.18.0",
    "eslint": "^4.16.0",
    "eslint-plugin-ember": "^5.0.3",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "^4.6.0",
    "mocha": "^5.0.0",
    "mocha-junit-reporter": "^1.16.0",
    "strip-indent": "^2.0.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
