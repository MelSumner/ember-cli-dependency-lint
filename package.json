{
  "name": "ember-cli-dependency-lint",
  "version": "1.0.0",
  "description": "Lint your app's addon dependencies, making sure you only have one version of each.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "lint": "eslint lib tests-node",
    "test": "yarn run lint && yarn run test-node && yarn run test-browser",
    "test-browser": "ember test",
    "test-node": "mocha tests-node/{unit,acceptance}/**/*-test.js"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "broccoli-plugin": "^1.3.0",
    "chalk": "^1.1.3",
    "semver": "^4.3.6"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai": "^3.5.0",
    "console-ui": "^1.0.3",
    "ember-ajax": "^2.5.4",
    "ember-cli": "^2.11.1",
    "ember-cli-addon-tests": "^0.6.0",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-babel": "^5.2.4",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.2",
    "ember-cli-htmlbars": "^1.0.10",
    "ember-cli-htmlbars-inline-precompile": "^0.3.3",
    "ember-cli-inject-live-reload": "^1.6.1",
    "ember-cli-qunit": "^3.1.1",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.11.1",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.6.3",
    "ember-resolver": "^2.1.1",
    "ember-source": "2.11",
    "eslint": "^3.15.0",
    "loader.js": "^4.0.10",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "strip-indent": "^2.0.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
